summary <- ddply(df_int
, .(time)
, function(x){
# D - 2. Run partial correlation
m.pcor=cor2pcor(cov(as.matrix(x[,c('meanprop_RNA','meanprop_ATAC','meanprop_k4')])))
k=n2kappa(nrow(x), p=3)
